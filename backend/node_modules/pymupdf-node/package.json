{
  "name": "pymupdf-node",
  "version": "0.0.4",
  "description": "PyMuPDF with JS",
  "author": "Artifex Software, Inc.",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "mupdf",
    "pymupdf",
    "node",
    "artifex"
  ],
  "scripts": {
    "prepare": "husky && npm run build",
    "eslint": "eslint --ext .ts",
    "format": "prettier --write .",
    "prebuild": "rimraf dist",
    "build": "tsup",
    "test": "vitest --test-timeout=30000",
    "test:run": "vitest run --test-timeout=30000"
  },
  "homepage": "https://pymupdf.readthedocs.io/en/latest/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pymupdf/pymupdf-node"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "pymupdf/**/*",
    "dist/**/*"
  ],
  "dependencies": {
    "pyodide": "^0.27.5"
  },
  "devDependencies": {
    "@types/node": "latest",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "tsup": "^8.5.0",
    "typescript": "latest",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run eslint"
    ]
  }
}
